{
  "manifest_version": 3,

  "name": "QRL - The Dynamic URL Queue",
  "short_name": "QRL",
  "description": "The mildly practical content queueing system for your browser.",
  "version": "0.5",

  "permissions": [
    "tabs",
    "contextMenus",
    "storage"
  ], 
  "icons": {
    "16": "images/icon-16.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "background": {
    "scripts": ["js/bootstrap.js"]
  },
  "browser_action": {
    "default_icon":{},
    "default_title": "QRL - The Dynamic URL Queue"
  }
}